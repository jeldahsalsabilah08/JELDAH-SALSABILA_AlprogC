# variabel adalah cara kita menyimpan atau mendeklarasikan nlai

x = 20
print(x)

y = x + 4
print("nilai y =",y)

# memberikan nilai/asigment
a = 10
b = 6
lebar = 50
print("nilai dari a =", a)
print("nilai dari b =", b)
print("nilai dari lebar =", lebar)

# penamaan variabel
jumlah_z = 9 #jika lebih dari satu kata, digabung dengan underscore
bet99 = 100000 #tidak boleh di awali dengan angka

#pemanggilan kedua
print("nilai dari a =", a)
a = 99
print("nilai dari a =", a)



nama = "jeldah talia"
nim = 23241079

print("Nama Saya =", nama)
print("NIM saya =", nim)